    <py_script># python script

import numpy as np
import itertools
from sklearn import tree
from pandas import DataFrame
from sklearn.externals import joblib
import os
import matplotlib.pyplot as plt


df = df.applymap(str)
Y = df['Attribution_Status'].values

df = df.drop('Attribution_Status', axis=1)

X = df.values


clf = tree.DecisionTreeClassifier()

from sklearn import preprocessing
le = preprocessing.LabelEncoder()


for i in range(X.shape[1]):
    X[:,i] = le.fit_transform(X[:,i])
Y = le.fit_transform(Y)

from sklearn.cross_validation import train_test_split
X_train, X_test, y_train, y_test = train_test_split(X, Y, test_size=0.33, random_state=42)

clf = clf.fit(X_train,y_train)
y_pred =clf.predict(X_test)



importances = clf.feature_importances_
features = df.columns


df1 = DataFrame({'features':features, 'importances': 
importances})


# save the model
# filename= 'C:\\share\\anya\\dt.NWR'

filename= 'C:\\Users\cmoreno\\Documents\\dt.NWR'

# C:\\Users\cmoreno\\Documents\\Projects\\Pre-Sales\\NetworkRails\\NWR Data and Solution\\

joblib.dump(clf,filename)



from sklearn.metrics import confusion_matrix

def plot_confusion_matrix(cm, classes,
                          normalize=False,
                          title='Confusion matrix',
                          cmap=plt.cm.Blues):
    """
    This function prints and plots the confusion matrix.
    Normalization can be applied by setting `normalize=True`.
    """
    plt.imshow(cm, interpolation='nearest', cmap=cmap)
    plt.title(title)
    plt.colorbar()
    tick_marks = np.arange(len(classes))
    plt.xticks(tick_marks, classes, rotation=45)
    plt.yticks(tick_marks, classes)

    if normalize:
        cm = cm.astype('float') / cm.sum(axis=1)[:, np.newaxis]
        print("Normalized confusion matrix")
    else:
        print('Confusion matrix, without normalization')

    print(cm)

    thresh = cm.max() / 2.
    for i, j in itertools.product(range(cm.shape[0]), range(cm.shape[1])):
        plt.text(j, i, cm[i, j],
                 horizontalalignment="center",
                 color="white" if cm[i, j] > thresh else "black")

    plt.tight_layout()
    plt.ylabel('True label')
    plt.xlabel('Predicted label')

# Compute confusion matrix
cnf_matrix = confusion_matrix(y_test, y_pred)
np.set_printoptions(precision=2)

# Plot non-normalized confusion matrix
plt.figure()
plot_confusion_matrix(cnf_matrix, classes=['Agreed','Disputed'],
                      title='Confusion matrix')

# plt.savefig('C:\\share\\anya\\confusion_matrix', bbox_inches='tight')

